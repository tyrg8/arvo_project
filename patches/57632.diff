         idx = index[tuple(z1)]
         s = sum(z1)
         arrg.append((idx,s))
     elif(easy_case1234(sizes)):
         z1 = grab_easy_case1234_code_point_size(sizes)
         idx = index[tuple(z1)]
         s = sum(z1)
         arrg.append((idx,s))
     else:
         # we are in error, use a bogus index
-        arrg.append((0,12))
+        arrg.append((209,12))
   print("const uint8_t utf8bigindex["+str(len(arrg))+"][2] = ")
   print(cpp_arrayarray_initializer(arrg), end=";\n")
 
